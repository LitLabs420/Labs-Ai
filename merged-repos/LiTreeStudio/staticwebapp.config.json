{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{json,xml,txt,js,css,png,jpg,jpeg,gif,ico,svg}"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
