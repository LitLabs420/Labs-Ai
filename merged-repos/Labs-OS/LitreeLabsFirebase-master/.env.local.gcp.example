# Example GCP-specific env values
# Copy sensitive values from your secure secret store into a local `.env.local` during development.

GCP_PROJECT_ID=your-gcp-project-id
GCP_CLIENT_EMAIL=service-account@your-project.iam.gserviceaccount.com
GCP_PRIVATE_KEY_ID=REDACTED_GCP_PRIVATE_KEY_ID

# BigQuery dataset
GCP_BIGQUERY_DATASET=litlabs_analytics

# Stripe integration
STRIPE_TO_GCP_SYNC=true
STRIPE_SYNC_INTERVAL=3600

# Logging
GCP_LOG_LEVEL=info
GCP_LOG_SINK_NAME=litlabs-error-logs
