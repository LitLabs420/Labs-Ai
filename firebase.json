{
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      ".firebaserc",
      ".git*",
      ".env*",
      "node_modules",
      "*.md",
      "dashboard-old-*",
      "financial.html",
      "admin.html",
      "functions/**"
    ],
    "redirects": [
      {
        "source": "/dashboard",
        "destination": "/dashboard.html",
        "type": 301
      }
    ]
  }
}
